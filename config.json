{
    "description": "Goofys plugin for Docker",
    "documentation": "https://github.com/haibin-fx/goofys-docker",
    "entrypoint": [
        "/usr/local/bin/goofys-docker"
    ],
    "env": [
        {
            "name": "AWS_ACCESS_KEY_ID",
            "settable": [
                "value"
            ]
        },
        {
            "name": "AWS_SECRET_ACCESS_KEY",
            "settable": [
                "value"
            ]
        },
        {
            "name": "AWS_NAMESPACE",
            "value": "goofys_",
            "settable": [
                "value"
            ]
        },
        {
            "name": "AWS_REGION",
            "settable": [
                "value"
            ]
        },
        {
            "name": "AWS_ENDPOINT",
            "settable": [
                "value"
            ]
        }
    ],
    "interface": {
        "socket": "goofys.sock",
        "types": [
            "docker.volumedriver/1.0"
        ]
    },
    "linux": {
        "capabilities": [
            "CAP_SYS_ADMIN"
        ],
        "devices": [
            {
                "path": "/dev/fuse"
            }
        ]
    },
    "mounts": [
        {
            "destination": "/mnt/cache",
            "options": [
                "rbind"
            ],
            "name": "cache",
            "source": "",
            "settable": [
                "source"
            ],
            "type": "bind"
        }
    ],
    "network": {
        "type": "host"
    },
    "propagatedmount": "/mnt/catfs"
}
